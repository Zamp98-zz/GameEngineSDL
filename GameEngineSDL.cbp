<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="GameEngineSDL" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/GameEngineSDL" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/GameEngineSDL" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-march=k8" />
			<Add option="-std=c++17" />
			<Add option="-std=c17" />
			<Add directory="$(#sdl2.include)/SDL2" />
			<Add directory="$(#sdl2image)" />
			<Add directory="$(#sdl2ttf)" />
		</Compiler>
		<Linker>
			<Add option="-O3" />
			<Add option="-lSDL2_image -g `sdl2-config --cflags --libs`" />
			<Add option="-lSDL2_ttf" />
			<Add library="mingw32" />
			<Add library="SDL2main" />
			<Add library="SDL2.dll" />
			<Add library="user32" />
			<Add library="gdi32" />
			<Add library="winmm" />
			<Add library="dxguid" />
			<Add directory="$(#sdl2.lib)" />
			<Add directory="$(#sdl2image.lib)" />
			<Add directory="$(#sdl2ttf)" />
		</Linker>
		<ExtraCommands>
			<Add after="XCOPY $(#sdl2)\bin\*.dll $(TARGET_OUTPUT_DIR) /D /Y" />
		</ExtraCommands>
		<Unit filename="Camera.h" />
		<Unit filename="Entity/cube.mtl" />
		<Unit filename="Entity/cube.obj" />
		<Unit filename="Fonts/arial.ttf" />
		<Unit filename="Math.h" />
